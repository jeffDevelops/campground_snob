    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    
    <div id="landing-header">
		 <p>- The best camping around the world, curated by campers like you -</p>
		 <a href="/campgrounds" id="landing-button" class="btn btn-lg btn-success">Plan your next trip!</a>
		 <h1>Campground<span class="snob">Snob</span></h1>
    </div>
    
    
    
